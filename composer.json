{
  "name": "miaoxing/app",
  "type": "miaoxing-plugin",
  "description": "Miaoxing App Plugin",
  "keywords": [
    "miaoxing",
    "app"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/plugin": "^0.11.0",
    "miaoxing/services": "^0.6.7"
  },
  "require-dev": {
    "miaoxing/dev": "^8.2.0"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\App\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\App\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "require-ci": {
      "miaoxing/dev": "miaoxing/dev as 8.x-dev",
      "miaoxing/phpstan-miaoxing": "miaoxing/phpstan-miaoxing as 1.x-dev",
      "miaoxing/plugin": "miaoxing/plugin as 0.10.x-dev",
      "wei/wei": "twinh/wei as 0.15.x-dev"
    }
  }
}
