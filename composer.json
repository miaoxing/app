{
  "name": "miaoxing/app",
  "type": "miaoxing-plugin",
  "description": "Miaoxing App Plugin",
  "keywords": [
    "miaoxing",
    "app"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/plugin": "^0.4.4",
    "miaoxing/services": "^0.3.9"
  },
  "require-dev": {
    "miaoxing/dev": "^6.4.0"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\App\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\App\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-types": [
      "miaoxing-plugin"
    ],
    "installer-paths": {
      "plugins/{$name}/": [
        "type:miaoxing-plugin"
      ]
    },
    "require-ci": {
      "miaoxing/dev": "miaoxing/dev as 6.x-dev"
    }
  }
}
