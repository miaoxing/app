{
  "name": "miaoxing/app",
  "type": "miaoxing-plugin",
  "keywords": [
    "miaoxing", "app"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/plugin": "dev-master@dev",
    "miaoxing/services": "dev-master@dev"
  },
  "require-dev": {
    "miaoxing/coding-standards": "^5.0.0@beta"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\App\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\App\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-types": [
      "miaoxing-plugin"
    ],
    "installer-paths": {
      "plugins/{$name}/": [
        "type:miaoxing-plugin"
      ]
    }
  }
}
